<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bikerentUI.admin.AdminController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label fx:id="Login">Not authorized</Label>
  <Button fx:id="UserControl" onAction="#onUserButtonClick" text="User control">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button fx:id="ShopControl" mnemonicParsing="false" onAction="#onShopButtonClick" text="Shop control">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
  <Button onAction="#onChangeButtonClick" text="Change password">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log out">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
</VBox>
