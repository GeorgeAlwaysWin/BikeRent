<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="248.0" prefWidth="269.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bikerentUI.worker.WorkerBookingControlController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox alignment="BASELINE_CENTER" layoutX="30.0" layoutY="111.0" prefHeight="0.0" prefWidth="200.0">
      <children>
         <Label text="Booking" />
         <TextField fx:id="Booking" />
         <Button mnemonicParsing="false" onAction="#onSearchButtonClick" text="🔍" />
      </children>
   </HBox>
   <Label fx:id="Rent" text="Enter booking code" />
  <Button fx:id="ApplyButton" disable="true" onAction="#onApplyButtonClick" prefHeight="25.0" prefWidth="150.0" text="Unavailable">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
</VBox>
