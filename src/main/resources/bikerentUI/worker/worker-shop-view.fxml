<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="248.0" prefWidth="269.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bikerentUI.worker.WorkerShopController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox alignment="BASELINE_CENTER" prefHeight="0.0" prefWidth="200.0">
      <children>
         <Label text="Shop">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin></Label>
        <ChoiceBox fx:id="Shops" prefWidth="150.0">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
        </ChoiceBox>
      </children>
   </HBox>
   <Label fx:id="Status" />
  <Button onAction="#onApplyButtonClick" text="Apply changes">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button mnemonicParsing="false" onAction="#onBackButtonClick" text="Back">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
</VBox>
